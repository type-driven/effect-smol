{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/refs/heads/main/cli/schemas/config-file.v1.json",
  "nodeModulesDir": "manual",
  "unstable": [
    "bare-node-builtins",
    "node-globals"
  ],
  "workspace": [
    "scratchpad",
    "scripts",
    "packages/*",
    "packages/ai/*",
    "packages/atom/*",
    "packages/sql/*",
    "packages/tools/*",
    "examples/*"
  ],
  "exclude": [
    "**/*.mjs",
    "**/*.cjs",
    "**/*.js",
    "**/*.d.ts",
    "**/vitest.*.ts",
    "**/.tsbuildinfo/",
    "**/node_modules/",
    "**/dist/",
    "ai-docs/",
    "bundle/",
    "examples/",
    "scripts/",
    "scratchpad/",
    "packages/*/dtslint/",
    "packages/*/benchmark/",
    "packages/ai",
    "packages/atom",
    "packages/effect/test/cluster/",
    "packages/opentelemetry/",
    "packages/platform-browser/",
    "packages/platform-bun/",
    "packages/platform-node/",
    "packages/platform-node-shared/",
    "packages/tools/",
    "packages/sql"
  ]
}
